        "doctrine\/orm": "^2.5.0",
        "phpdocumentor\/reflection-docblock": "^2.0",
        "doctrine\/cache": "^1.4.1",
        "guzzlehttp\/guzzle": "^6.0.2",
        "jms\/serializer": "^1.0",
        "monolog\/monolog": "^1.16.0",
